/* From compare result of Nougat dtb and Oreo dtb */
&other_ext_mem {
	reg = <0x00 0x85b00000 0x00 0xd00000>;
};

&eeprom2 {
	qcom,cam-power-seq-cfg-val = <0 0 1 1 1 24000000 1 1>;
	qcom,cam-power-seq-delay = <1 1 5 5 5 1 10 10>;
	qcom,cam-power-seq-type =
		"sensor_gpio",
		"sensor_gpio",
		"sensor_vreg",
		"sensor_vreg",
		"sensor_vreg",
		"sensor_clk",
		"sensor_gpio",
		"sensor_gpio";
	qcom,cam-power-seq-val =
		"sensor_gpio_reset",
		"sensor_gpio_standby",
		"cam_vana",
		"cam_vdig",
		"cam_vio",
		"sensor_cam_mclk",
		"sensor_gpio_reset",
		"sensor_gpio_standby";
	qcom,cam-vreg-op-mode = <105000 80000 100000>;
};

&eeprom3 {
	qcom,cam-power-seq-cfg-val = <0 0 1 1 1 24000000 1 1>;
	qcom,cam-power-seq-delay = <1 1 5 5 5 1 10 10>;
	qcom,cam-power-seq-type =
